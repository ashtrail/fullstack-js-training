    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "eslint": "^7.30.0",
    "factory-girl": "^5.0.4",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "prettier": "2.3.2",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3"
  },
  "jest": {
    "testEnvironment": "node",
